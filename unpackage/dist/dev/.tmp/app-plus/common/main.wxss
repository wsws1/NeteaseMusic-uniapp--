/* 半像素  top */
.harf-px-top {
	position: relative;
	border: none;
}
.harf-px-top:after {
	content: "";
	position: absolute;
	top: 0;
	background: #E5E5E5;
	width: 100%;
	height: 1rpx;
	-webkit-transform: scaleY(0.5);
	        transform: scaleY(0.5);
}

